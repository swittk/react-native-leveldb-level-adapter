{
  "name": "react-native-leveldb-level-adapter",
  "version": "0.1.0-alpha3",
  "description": "level adapter for react-native-leveldb",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-leveldb-level-adapter.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://swittssoftware.com",
  "author": "Switt Kongdachalert <_@_.com> (https://swittssoftware.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://swittssoftware.com/issues"
  },
  "homepage": "https://swittssoftware.com#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "17.0.2",
    "@types/react-native": "0.68.2",
    "@types/tape": "^4.13.2",
    "commitlint": "^11.0.0",
    "path": "^0.12.7",
    "pod-install": "^0.1.0",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-builder-bob": "^0.18.0",
    "release-it": "^14.2.2",
    "stream": "^0.0.2",
    "streams": "^0.0.9",
    "tape": "^5.5.3",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@switt/react-native-leveldb": "3.0.6-alpha1",
    "abstract-level": "^1.0.3",
    "events": "^3.3.0",
    "react-native-fs": "^2.20.0"
  }
}
